{
  "name": "pddl-common",
  "description": "PDDL node module without dependency on the VS Code.",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/jan-dolejsi/vscode-pddl"
  },
  "scripts": {
    "pretest": "tsc",
    "test": "mocha -- out/test/*.js",
    "lint": "eslint --config ../client/.eslintrc.js src test --ext ts",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "vscode:prepublish": "npm run compile",
    "update-vscode": "node ./node_modules/vscode/bin/install"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "open": "^7.0.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@types/node": "10.14.11",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.2",
    "@types/mocha": "^5.2.7",
    "@types/request": "^2.48.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@types/uuid": "^3.4.6",
    "mocha": "^6.2.1",
    "eslint": "^6.8.0",
    "typescript": "^3.7.3"
  }
}
